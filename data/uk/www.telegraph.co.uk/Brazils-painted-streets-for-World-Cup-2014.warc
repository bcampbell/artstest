			    <script type="text/javascript">
			    
			    /* 	
				THG function for selection of countries and regions.
				It sets the form 'action' based on the selected country and/or region.
				
			   	This script is optional. Without this script the search will default to 'all-regions' ( all countries and all regions )
			  	If this script is not used with the quick search widget,
			   		the 'action can be set manually and  the country and region drop downs can be hidden/removed.
					
					When setting a form 'action' manually, the format is /country/region
					
			*/
			document.getElementsByClassName = function(cl) { var retnode = []; var myclass = new RegExp('\\b'+cl+'\\b'); var elem = this.getElementsByTagName('*'); 
			for (var i = 0; i < elem.length; i++) { var classes = elem[i].className; if (myclass.test(classes)) retnode.push(elem[i]); } return retnode; }; 

			(function() {
			function thg$(id) { return document.getElementById(id); }
			var thg_countryselect = thg$("thg_countrylist"),  thg_availregions=thg$("thg_regions").innerHTML, thg_submit = thg$("thg_submit"), 
			thg_regionselect = thg$("thg_regionlist"), thg_searchform = thg$("thg_searchform");
				/* listen */
				thg_countryselect.onchange = function() { 
				if(thg_regionselect) {
					thg_regions.innerHTML="";
					thg_regions.innerHTML=thg_availregions; 
					var thg_sel = "thg_"+this.options[this.selectedIndex].value.toLowerCase();
					var regionsToHideArray = new Array();
					var regionOptgroups = thg$("thg_regionlist").getElementsByTagName("optgroup");
						for (i=0; i<regionOptgroups.length;i++) { 
						 var cId = regionOptgroups[i].id; if(cId!=thg_sel) {regionsToHideArray[i]=cId;} }
						for (ir=0; ir<=regionsToHideArray.length;ir++) { 
						 if(thg$(regionsToHideArray[ir])) {var no = thg$(regionsToHideArray[ir]); no.parentNode.removeChild(no);} }	
				}
				}
			    /* submit */
				thg_searchform.onsubmit = function() { 
				var thg_regionselectn = thg$("thg_regionlist");
				var selAction = this.action;
				/* get region or country */ 
				if(thg_regionselectn) { 
				var selRegion = thg_regionselectn.options[thg_regionselectn.selectedIndex].value.toLowerCase();
				selAction = selAction.substring(0,selAction.indexOf("all-regions"))+selRegion; 
				if(selAction.indexOf("all-regions")>0) {selAction = selAction.substring(0,selAction.indexOf("all-regions")-1); }
				} else {
					var thg_countryselectn = thg$("thg_countrylist");
					if(thg_countryselectn) { 
					var selCountry = thg_countryselectn.options[thg_countryselectn.selectedIndex].value.toLowerCase(); 
					selAction = selAction.substring(0,selAction.indexOf("all-regions"))+selCountry; 
					}
				}
				

				/* build query string */
				
				var thg_adults = document.getElementById("thg_adultlist").value;
				var thg_startdate = document.getElementById("thg_startdate").value;
					if(thg_startdate.length>0) { thg_startdate= "&start="+thg_startdate; }



				window.open(selAction+"?adults="+thg_adults+thg_startdate);
				return false;
				}
			})();


			    </script>
