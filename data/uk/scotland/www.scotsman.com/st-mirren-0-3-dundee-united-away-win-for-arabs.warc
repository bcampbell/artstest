 } 

 #wowf header h1{

 padding-top:25px; margin-bottom:8px;

 background: url("http://editorial.jpress.co.uk/edinburghFest/logo_s.png") no-repeat left top;

 }

 #wowf header h1 span{

 font-family: "OpenSans-Light", Arial, sans-serif; font-size:85%;

 }

 #wowf header h2{ 

 padding-top:1px;

 border-top: 2px solid #fff; border-bottom: 2px solid #fff;

 font-size:1.2em; line-height:130%; 

 } 

 #wowf header h3{ 

 margin:5px 0 0;

 font-size: 0.7em;

 background: url("http://editorial.jpress.co.uk/edinburghFest/scots_m.png") no-repeat 117px 1px;

 }

 #wowf .lead{ 

 padding:15px 10px; 

 font-size: 1.05em; line-height:130%; 

 } 

 #wowf .btnBar{ 

 padding: 5px 0; 

 }

 #wowf .btnBar .wowfClose, #wowf .btnBar a{ 

 display:inline-block; 

 padding:10px; width:260px; margin:0 0 20px; 

 font-size:1.3em; 

 } 

 }

 </style>

 

<div id="wowf">

	<div class="wowfInner">

		<header>

			<h1>UNMISSABLE SHOWS.<br />

			UNMISSABLE COVERAGE.<br />

			MAKE THE MOST OF THE FESTIVAL<br />

				<span>(BEFORE YOU MISS IT)</span>

			</h1>

			<h2>#WOWFEST</h2>

			<h3>In partnership with</h3>

		</header>

		<p class="lead">Complete coverage of the festivals. Guides. Reviews. Listings. Offers</p>

		<div class="btnBar">

			<a href="http://www.wow247.co.uk/festivals/edinburgh-festival/?WT.mc_id=Scotsman_EdinFest&amp;utm_source=Scotsman&amp;utm_medium=ModalWindow&amp;utm_campaign=EdinFest" title="lets go to the festival site" title="lets go to the festival site">Lets Go!</a>

			<p class="wowfClose" title="no thanks">No Thanks</p> 

		</div>

	</div><!-- // end tdfInner -->

</div>

 

<script>

	$(document).ready(function(){

		setTimeout(function(){

			if ($("#wowf").length > 0) {

				if (document.cookie.indexOf("wowfCookieA") >= 0){

					$("#wowf").hide();

				}else{

					$('#wowf').lightbox_me({

					    zIndex: 10002,

						overlayCSS: ({background: "#000", opacity: "0.85", position: "fixed"}),

						closeSelector: (".wowfClose"),

					    closeClick: false

					});

					$(".wowfClose, .btnBar a").click(function(){

						var date = new Date();

						var minutes = 4320; //60 * 24 = 1440 * 3 days = 4320

						date.setTime(date.getTime() + (minutes * 60 * 1000));

						jQuery.cookie("wowfCookieA", 1, {expires : date});

					});

				}

			}

		},100);

	});

</script>
				
			
      
      
          
        
        
                        

